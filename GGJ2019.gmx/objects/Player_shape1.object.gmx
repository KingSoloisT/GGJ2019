if ((key_thrust_held) &amp;&amp; (canPush) &amp;&amp; (!isMoving))
{
    boost += boosterPower;
    show_debug_message(boost);
}

if (key_thrust_release &amp;&amp; canPush &amp;&amp; (!isMoving))
{
    isMoving = true;
}

dist = distance_to_object(home);
if (dist &lt;= 100)
{
    volume = 1;
}
else if (dist &lt;= 1500)
{
    volume = 0.9;
}
else if (dist &lt;= 2000)
{
    volume = 0.8;
}
else if (dist &lt;= 2500)
{
    volume = 0.7;
}
else if (dist &lt;= 3000)
{
    volume = 0.6;
}
else if (dist &lt;= 3500)
{
    volume = 0.5;
}
else if (dist &lt;= 4000)
{
    volume = 0.4;
}
else if (dist &lt;= 4500)
{
    volume = 0.3;
}
else if (dist &lt;= 5000)
{
    volume = 0.2;
}
else if (dist &lt;= 5500)
{
    volume = 0.1;
}
else if (dist &lt;= 6000)
{
    volume = 0.05;
}
else
{
    volume = 0;
}

if (isMoving)
{
    motion_add(dir+90, boost/3);
    audio_sound_gain(sound,volume,1);
    audio_play_sound(sound,30,false);
    moves++;
    boost = 0;
    isMoving = false;
}
else
{
    speed = speed - slowSpeed;
}
